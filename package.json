{
  "name": "core-home",
  "productName": "core: Home",
  "version": "1.0.2",
  "description": "Electron Hydrogen UI app",
  "main": "main.js",
  "scripts": {
    "start": "electron-forge start",
    "dev": "nodemon --exec electron-forge start",
    "build": "electron-forge make"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "discord-rpc": "^4.0.1",
    "electron-squirrel-startup": "^1.0.1",
    "electron-updater": "^5.0.0",
    "node-fetch": "^2.6.7",
    "nodemon": "^3.1.9",
    "semver": "^7.3.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.3.0",
    "@electron-forge/maker-dmg": "^7.3.0",
    "@electron-forge/maker-zip": "^7.3.0",
    "electron": "^35.1.2",
    "nodemon": "^3.1.9"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "appId": "com.titanbound.corehome",
        "asar": true,
        "name": "Core Home",
        "dir": "/Users/frank/corehome",
        "ignore": [
          "/.git",
          "/node_modules/.bin",
          "/node_modules/.cache"
        ],
        "arch": [
          "x64",
          "arm64"
        ]
      },
      "makers": [
        {
          "name": "@electron-forge/maker-dmg",
          "platforms": [
            "darwin"
          ],
          "config": {
            "format": "ULFO"
          }
        }
      ]
    }
  },
    "repository": {
    "type": "git",
    "url": "https://github.com/titanbound/core-home-hydrogen.git"
  },
  "publish": {
    "provider": "github",
    "releaseType": "release"
  }
}
